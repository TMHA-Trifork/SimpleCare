{
  "name": "simplecare-webapplication",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "openApi": "npx openapi-typescript ../OpenApi.json --output src/types/api-types.ts",
